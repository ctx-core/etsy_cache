{
	"name": "@ctx-core/etsy_cache",
	"version": "10.0.0",
	"description": "cache data for an etsy account",
	"author": "Brian Takit",
	"homepage": "https://github.com/ctx-core/etsy_cache#readme",
	"type": "module",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-etsy": "./bin/cache-etsy.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"start": "./bin/cache-etsy.mjs"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.44.0",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.85",
		"@ctx-core/dom": "workspace:^11.0.40",
		"@ctx-core/etsy": "^3.0.19",
		"@ctx-core/fetch": "workspace:^11.0.93",
		"@ctx-core/function": "workspace:^20.2.12",
		"@ctx-core/object": "workspace:^22.0.0",
		"@ctx-core/sleep": "workspace:^8.0.24",
		"@ctx-core/store": "workspace:^27.0.46",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"typescript": "next"
	}
}
