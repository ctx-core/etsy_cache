{
	"name": "@ctx-core/etsy_cache",
	"version": "16.1.65",
	"description": "cache data for an etsy account",
	"author": "Brian Takita",
	"homepage": "https://github.com/ctx-core/etsy_cache#readme",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./bin",
		"./_types",
		"./etsy_cache",
		"./etsy_cache__listing_a",
		"./etsy_cache__listing_image_a",
		"./etsy_listing_a",
		"./etsy_listing_image_a",
		"./etsy_listing_o",
		"./loading__s3__etsy_listing_a",
		"./loading__s3__etsy_listing_image_a",
		"./s3__etsy_listing_a",
		"./s3__etsy_listing_image_a"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-etsy": "./bin/cache-etsy.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.462.0",
		"@ctx-core/dom": "^15.1.33",
		"@ctx-core/env": "^17.4.36",
		"@ctx-core/etsy": "^3.0.19",
		"@ctx-core/fetch-undici": "^3.1.58",
		"@ctx-core/function": "^21.22.28",
		"@ctx-core/nanostores": "^9.0.18",
		"@ctx-core/object": "^31.1.14",
		"@ctx-core/sleep": "^8.2.33"
	},
	"devDependencies": {
		"@aws-sdk/node-config-provider": "^3.374.0",
		"@aws-sdk/types": "^3.460.0",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.6.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/etsy_cache.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/etsy_cache/issues"
	},
	"sideEffects": false
}