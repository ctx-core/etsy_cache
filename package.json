{
	"name": "@ctx-core/etsy_cache",
	"version": "14.0.31",
	"description": "cache data for an etsy account",
	"author": "Brian Takit",
	"homepage": "https://github.com/ctx-core/etsy_cache#readme",
	"type": "module",
	"types": "./src/index.d.ts",
	"exports": {
		".": "./src/index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-etsy": "./bin/cache-etsy.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build && npm run test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsm node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.67.0",
		"@ctx-core/dom": "workspace:^11.1.10",
		"@ctx-core/etsy": "^3.0.19",
		"@ctx-core/fetch-undici": "workspace:^1.2.15",
		"@ctx-core/function": "workspace:^20.6.1",
		"@ctx-core/nanostores": "workspace:^1.0.11",
		"@ctx-core/object": "workspace:^22.1.4",
		"@ctx-core/sleep": "workspace:^8.0.37"
	},
	"devDependencies": {
		"c8": "^7.11.0",
		"check-dts": "^0.6.6",
		"tsm": "^2.2.1",
		"typescript": "next",
		"uvu": "^0.5.3"
	},
	"svelte": "./src/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/etsy_cache.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/etsy_cache/issues"
	},
	"sideEffects": false
}
