{
	"name": "@ctx-core/etsy_cache",
	"version": "11.0.5",
	"description": "cache data for an etsy account",
	"author": "Brian Takit",
	"homepage": "https://github.com/ctx-core/etsy_cache#readme",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-etsy": "./bin/cache-etsy.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "tsc src/index.ts --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"start": "./bin/cache-etsy.mjs"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.44.0",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.91",
		"@ctx-core/dom": "workspace:^11.0.44",
		"@ctx-core/etsy": "^3.0.19",
		"@ctx-core/fetch": "workspace:^11.0.97",
		"@ctx-core/function": "workspace:^20.2.16",
		"@ctx-core/object": "workspace:^22.0.4",
		"@ctx-core/sleep": "workspace:^8.0.28",
		"@ctx-core/store": "workspace:^27.0.51"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"svelte": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/etsy_cache.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/etsy_cache/issues"
	}
}
