{
	"name": "@ctx-core/etsy_cache",
	"version": "15.0.320",
	"description": "cache data for an etsy account",
	"author": "Brian Takita",
	"homepage": "https://github.com/ctx-core/etsy_cache#readme",
	"type": "module",
	"types": "./src/index.d.ts",
	"exports": {
		".": "./src/index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-etsy": "./bin/cache-etsy.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.385.0",
		"@ctx-core/dom": "^14.1.58",
		"@ctx-core/env": "^17.2.128",
		"@ctx-core/etsy": "^3.0.19",
		"@ctx-core/fetch-undici": "^2.6.97",
		"@ctx-core/function": "^21.19.0",
		"@ctx-core/nanostores": "^6.0.1",
		"@ctx-core/object": "^28.0.4",
		"@ctx-core/sleep": "^8.1.19"
	},
	"devDependencies": {
		"@aws-sdk/node-config-provider": "^3.374.0",
		"@aws-sdk/types": "^3.378.0",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^3.12.7",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"svelte": "./src/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/etsy_cache.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/etsy_cache/issues"
	},
	"sideEffects": false
}