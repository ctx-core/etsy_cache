{
	"name": "@ctx-core/etsy_cache",
	"version": "9.0.76",
	"description": "cache data for an etsy account",
	"author": "Brian Takit",
	"homepage": "https://github.com/ctx-core/etsy_cache#readme",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-etsy": "./bin/cache-etsy.mjs"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run generate_ctx_I_file && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file",
		"prepublishOnly": "npm run build",
		"start": "./bin/cache-etsy.mjs"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.28.0",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.63",
		"@ctx-core/dom": "workspace:^11.0.22",
		"@ctx-core/etsy": "^3.0.19",
		"@ctx-core/fetch": "workspace:^11.0.58",
		"@ctx-core/function": "workspace:^20.1.3",
		"@ctx-core/object": "workspace:^21.0.17",
		"@ctx-core/sleep": "workspace:^8.0.12",
		"@ctx-core/store": "workspace:^27.0.25",
		"esm": "^3.2.25",
		"svelte": "3.42.4"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.1.2",
		"typescript": "^4.4.2"
	}
}